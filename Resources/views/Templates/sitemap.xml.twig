<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for article in articles %}
        <url>
            <loc>{{ url(article.routeName) }}</loc>
            <lastmod>{{ article.updatedAt.format(constant('DATE_ATOM')) }}</lastmod>
        </url>
    {% endfor %}
</urlset>