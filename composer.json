{
    "name": "vince/cms-bundle",
    "type": "symfony-bundle",
    "description": "Basic CMS features for Symfony 2.3",
    "keywords": ["CMS", "Vince"],
    "homepage": "https://github.com/vincentchalamon/VinceCmsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent CHALAMON",
            "email": "vincentchalamon@gmail.com"
        }
    ],
    "require": {
        "doctrine/doctrine-fixtures-bundle": "2.1.*",
        "gedmo/doctrine-extensions": "2.3.5",
        "liip/functional-test-bundle": "1.0.*",
        "jms/serializer-bundle": "0.13.*",
        "knplabs/knp-paginator-bundle": "2.4.*"
    },
    "suggest": {
        "vince/cms-sonata-admin-bundle": "to manage VinceCms with Sonata Admin",
        "friendsofsymfony/elastica-bundle": "to use elastisearch"
    },
    "autoload": {
        "psr-0": { "Vince\\Bundle\\CmsBundle": "" }
    },
    "target-dir": "Vince/Bundle/CmsBundle",
    "version": "0.1.0-beta"
}
