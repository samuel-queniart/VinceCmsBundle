{
    "name": "vince/cms-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Basic CMS features for Symfony 2.3",
    "keywords": ["CMS", "Vince"],
    "homepage": "http://vincentchalamon.github.io/VinceCmsBundle/",
    "autoload": {
        "psr-0": { "Vince\\Bundle\\CmsBundle": "" }
